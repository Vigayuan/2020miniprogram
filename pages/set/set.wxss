/* pages/set/set.wxss */
.wrapper{
  padding-top: 35rpx;
  background: #f5f5f5;
}
.wrapper .innerWrapper {
  box-shadow: 0 0 5rpx #dddddd;
}
.wrapper .innerWrapper .item{
  box-sizing: border-box;
  width: 100%;
  height: 80rpx;
  display: flex;
  justify-content: space-between;
  font-size: 32rpx;
  line-height: 80rpx;
  color: #666666;
  padding: 0 20rpx;
  background: #ffffff;
  border-bottom: 1rpx solid #dddddd;
}
.wrapper .innerWrapper .item .left{
  width: 30%;
}
.wrapper .innerWrapper .item .phone{
  display: flex;
}